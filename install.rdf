    <!-- Front End MetaData -->
    <em:name>about:me</em:name>
    <em:description>The about:me page provides a fun way to see personalized patterns in browser usage, including trends in browsing history and download activity.</em:description>
    <em:creator>Margaret Leibovic (margaret.leibovic@gmail.com)</em:creator>
    <em:contributor>Ferenc Sz√©kely (ferenc.szekely@gmail.com)</em:contributor>
